/*弹性布局 warp  start  end  between  around  center  middle*/

.flex {
  display: flex;
}

.shrink {
  flex-shrink: 0;
}

.warp {
  display: flex;
  flex-wrap: wrap;
}

.start {
  display: flex;
  justify-content: flex-start;
}

.end {
  justify-content: flex-end;
  display: flex;
}

