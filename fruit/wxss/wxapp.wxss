/* 边框*/

.b {
  position: relative;
}

.b::after {
  content: "";
  pointer-events: none;
  box-sizing: border-box;
  position: absolute;
  width: 200%;
  height: 200%;
  left: 0;
  top: 0;
  border-radius: 36rpx;
  border-top: 1rpx solid #d8dce5;
  border-bottom: 1rpx solid #d8dce5;
  border-right: 1rpx solid #d8dce5;
  border-left: 1rpx solid #d8dce5;
  transform: scale(0.5, 0.5);
  transform-origin: 0 0;
}

